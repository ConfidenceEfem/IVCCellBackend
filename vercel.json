{
    "version": 2,
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "dist/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ]
  }